{
  "name": "koa-router-swagger-decorators",
  "version": "5.0.29",
  "description": "A Typescript decorative library for Koa-router integrating swagger.",
  "main": "lib/index.js",
  "module": "src/index.ts",
  "scripts": {
    "start": "ts-node-dev --prefer-ts --debug --transpile-only --inspect=0.0.0.0:6788 ./tests/supports/app/index.ts",
    "prepare": "git init && git config core.hooksPath .githooks",
    "test": "cross-env-shell NODE_ENV=unit-test PORT=10000 mocha -r ts-node/register **/tests/spec/**/*.spec.ts --timeout 200000 --exit",
    "coverage": "nyc npm run test",
    "build": "tsc",
    "preversion": "npm test",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "koa-router",
    "swagger",
    "decorator",
    "typescript",
    "router",
    "koa"
  ],
  "author": "Colin <mail@luozhihua.com>",
  "license": "ISC",
  "types": "./declarations",
  "gitHooks": {},
  "dependencies": {
    "@types/bytes": "^3.1.0",
    "ajv": "^7.0.2",
    "ajv-formats": "^1.5.1",
    "ajv-keywords": "^4.0.0-beta.3",
    "bytes": "^3.1.0",
    "image-size": "^0.9.3",
    "koa-swagger-decorator": "^1.6.8",
    "lodash.merge": "^4.6.2",
    "mime": "^1.6.0",
    "multimatch": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/faker": "^5.1.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^13.9.2",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cross-env": "^5.2.1",
    "faker": "^5.1.0",
    "form-data": "^3.0.0",
    "istanbul": "^0.4.5",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "lint-staged": "^8.2.1",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "nyc": "^13.3.0",
    "ts-lint": "^4.5.1",
    "ts-node-dev": "^1.1.1",
    "tslint": "^5.20.1",
    "typescript": "^4.1.3",
    "yorkie": "^2.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
